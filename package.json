{
  "name": "api-unwork",
  "version": "0.0.0",
  "description": "Global REST API Job Scouting Service",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shba007/api-unwork.git"
  },
  "type": "module",
  "private": true,
  "volta": {
    "node": "22.12.0"
  },
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "scripts": {
    "prepare": "nitro prepare && prisma generate",
    "dev": "nitro dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier .  --write",
    "commit": "oco",
    "task:scout:remotive": "nitro task run scout:remotive --payload \"{\\\"limit\\\": 100}\"",
    "task:filter": "nitro task run filter --payload \"{\\\"category\\\": \\\"Project Management\\\", \\\"daysAgo\\\": 7, \\\"sortBy\\\": \\\"recency\\\"}\"",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:show": "prisma studio",
    "build": "nitro build",
    "preview": "node --env-file=.env .output/server/index.mjs",
    "start": "node .output/server/index.mjs",
    "clean": "rm -rf .nitro .output"
  },
  "dependencies": {
    "@prisma/client": "6.0.1",
    "date-fns": "^4.1.0",
    "nitropack": "^2.10.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "prisma": "^6.0.1",
    "untun": "^0.1.3"
  }
}